{
  "datasets": [
    {
      "name": "ds_aum_counter",
      "displayName": "Latest AUM Counter",
      "queryLines": [
        "SELECT aum_billion FROM sas_demo.saswata_sengupta_agneticportfoliomanager.gold_latest_aum_counter "
      ],
      "columns": [
        {
          "displayName": "AUM (Billions USD)",
          "expression": "`aum_billion`"
        }
      ]
    },
    {
      "name": "ds_monthly_turnover_counter",
      "displayName": "Monthly Turnover Counter",
      "queryLines": [
        "SELECT DATE_TRUNC('MONTH', date) AS month_start, AVG(turnover_pct) AS monthly_turnover_pct FROM sas_demo.saswata_sengupta_agneticportfoliomanager.gold_execution_costs_daily WHERE DATE_TRUNC('MONTH', date) = DATE_TRUNC('MONTH', add_months(current_date(), -1)) GROUP BY DATE_TRUNC('MONTH', date) "
      ],
      "columns": [
        {
          "displayName": "Month Start",
          "expression": "`month_start`"
        },
        {
          "displayName": "Monthly Turnover (%)",
          "expression": "`monthly_turnover_pct`"
        }
      ]
    },
    {
      "name": "ds_trailing_slippage_counter",
      "displayName": "Trailing 30d Execution Costs",
      "queryLines": [
        "SELECT trailing30_avg_slippage_bps, trailing30_avg_quote_spread_bps FROM sas_demo.saswata_sengupta_agneticportfoliomanager.gold_trailing_30d_execution_costs "
      ],
      "columns": [
        {
          "displayName": "Trailing30 Avg Slippage (bps)",
          "expression": "`trailing30_avg_slippage_bps`"
        },
        {
          "displayName": "Trailing30 Avg Quote Spread (bps)",
          "expression": "`trailing30_avg_quote_spread_bps`"
        }
      ]
    },
    {
      "name": "ds_open_breaches_7d_counter",
      "displayName": "Open Breaches 7 Days",
      "queryLines": [
        "SELECT breach_count_7d FROM sas_demo.saswata_sengupta_agneticportfoliomanager.gold_open_breaches_7d "
      ],
      "columns": [
        {
          "displayName": "Breaches (7d)",
          "expression": "`breach_count_7d`"
        }
      ]
    },
    {
      "name": "ds_vector_nvda_timeseries",
      "displayName": "Volatility Vector Shift (NVDA)",
      "queryLines": [
        "SELECT date, ticker, vector_shift_ratio_volatility, event_window FROM sas_demo.saswata_sengupta_agneticportfoliomanager.gold_vector_timeseries WHERE ticker = 'NVDA' ORDER BY date "
      ],
      "columns": [
        {
          "displayName": "Date",
          "expression": "`date`"
        },
        {
          "displayName": "Ticker",
          "expression": "`ticker`"
        },
        {
          "displayName": "Volatility Shift Ratio",
          "expression": "`vector_shift_ratio_volatility`"
        },
        {
          "displayName": "Event Window Flag",
          "expression": "`event_window`"
        }
      ]
    },
    {
      "name": "ds_weekly_breach_counts",
      "displayName": "Weekly Breach Counts by Type",
      "queryLines": [
        "SELECT week_start, sleeve, limit_type, weekly_breach_count FROM sas_demo.saswata_sengupta_agneticportfoliomanager.gold_weekly_breach_counts_by_type ORDER BY week_start, sleeve, limit_type "
      ],
      "columns": [
        {
          "displayName": "Week Start",
          "expression": "`week_start`"
        },
        {
          "displayName": "Sleeve",
          "expression": "`sleeve`"
        },
        {
          "displayName": "Limit Type",
          "expression": "`limit_type`"
        },
        {
          "displayName": "Weekly Breach Count",
          "expression": "`weekly_breach_count`"
        }
      ]
    },
    {
      "name": "ds_exec_costs_vs_spreads",
      "displayName": "Execution Costs vs Spreads Timeseries",
      "queryLines": [
        "SELECT date, sleeve, ticker, avg_slippage_bps, avg_quote_spread_bps FROM sas_demo.saswata_sengupta_agneticportfoliomanager.gold_execution_costs_vs_spreads_timeseries ORDER BY date, sleeve, ticker "
      ],
      "columns": [
        {
          "displayName": "Date",
          "expression": "`date`"
        },
        {
          "displayName": "Sleeve",
          "expression": "`sleeve`"
        },
        {
          "displayName": "Ticker",
          "expression": "`ticker`"
        },
        {
          "displayName": "Avg Slippage (bps)",
          "expression": "`avg_slippage_bps`"
        },
        {
          "displayName": "Avg Quote Spread (bps)",
          "expression": "`avg_quote_spread_bps`"
        }
      ]
    },
    {
      "name": "ds_policy_change_log",
      "displayName": "Risk Policy Change Log",
      "queryLines": [
        "SELECT change_date_date, scope_sleeve, scope_sector, limit_type, old_threshold, new_threshold, change_magnitude, notes FROM sas_demo.saswata_sengupta_agneticportfoliomanager.silver_risk_policy_changes ORDER BY change_date_date DESC "
      ],
      "columns": [
        {
          "displayName": "Change Date",
          "expression": "`change_date_date`"
        },
        {
          "displayName": "Scope Sleeve",
          "expression": "`scope_sleeve`"
        },
        {
          "displayName": "Scope Sector",
          "expression": "`scope_sector`"
        },
        {
          "displayName": "Limit Type",
          "expression": "`limit_type`"
        },
        {
          "displayName": "Old Threshold",
          "expression": "`old_threshold`"
        },
        {
          "displayName": "New Threshold",
          "expression": "`new_threshold`"
        },
        {
          "displayName": "Change Magnitude",
          "expression": "`change_magnitude`"
        },
        {
          "displayName": "Notes",
          "expression": "`notes`"
        }
      ]
    },
    {
      "name": "ds_pnl_timeseries",
      "displayName": "PnL Timeseries",
      "queryLines": [
        "SELECT date, sleeve, cumulative_pnl_pct FROM sas_demo.saswata_sengupta_agneticportfoliomanager.gold_pnl_timeseries ORDER BY date, sleeve "
      ],
      "columns": [
        {
          "displayName": "Date",
          "expression": "`date`"
        },
        {
          "displayName": "Sleeve",
          "expression": "`sleeve`"
        },
        {
          "displayName": "Cumulative PnL (%)",
          "expression": "`cumulative_pnl_pct`"
        }
      ]
    }
  ],
